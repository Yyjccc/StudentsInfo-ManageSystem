<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--命名空间-->
<mapper namespace="Mode.Mapper.CodeMapper">
<select id="changeCodes" resultType="Mode.enity.Code">
    select * from changeCode
</select>
    <select id="rewardCode" resultType="Mode.enity.Code">
        select * from rewardCode
    </select>
    <select id="punishmentCode" resultType="Mode.enity.Code">
        select * from punishmentCode
    </select>
    <select id="classCode" resultType="Mode.enity.Code">
        select * from classCode
    </select>
    <select id="departmentCode" resultType="Mode.enity.Code">
        select * from departmentCode
    </select>
</mapper>